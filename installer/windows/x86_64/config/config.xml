<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>Dialogue</Name>
    <Version>0.1</Version>
    <Title>Dialogue Chat</Title>
    <Publisher>The Dialogue Project</Publisher>
    <ProductUrl>https://github.com/adolby/Dialogue/</ProductUrl>
    <InstallerWindowIcon>dialogue</InstallerWindowIcon>
    <InstallerApplicationIcon>dialogue</InstallerApplicationIcon>
    <Logo>logo.png</Logo>
    <AllowSpaceInPath>true</AllowSpaceInPath>
    <StartMenuDir>Dialogue</StartMenuDir>
    <TargetDir>@RootDir@/Program Files/@ProductName@</TargetDir>
    <AdminTargetDir>@RootDir@/Program Files/@ProductName@</AdminTargetDir>
</Installer>
